{
    "manifest_version": 2,
  
    "name": "Demo typer",
    "description": "This extension will type your code/text on any key press",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "index.html"
    },
    "background":{
        "scripts": ["background.js"]
      },
    "permissions": [
     "activeTab"
     ]
  }
  